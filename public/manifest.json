      "icons": [
        {
          "src": "/favicon-192x192.png",
          "sizes": "192x192",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "오늘 일정",
      "short_name": "오늘",
      "description": "오늘의 일정 보기",
      "url": "/calendar?view=today",
